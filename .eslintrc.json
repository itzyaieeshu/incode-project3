{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-console":"off",
        "no-plusplus":"off",
        "indent":"off",
        "max-len":"off",
        "no-param-reassign":"warn"
    }
}
